{
  "name": "exif-date",
  "version": "1.2.0",
  "description": "Parse an EXIF date string into a `Date` object",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "build": "rm -rf dist && tsc",
    "test-spec": "blue-tape 'dist/**/*.spec.js' | tap-spec",
    "test-cov": "istanbul cover --print none -x '*.spec.js' node_modules/blue-tape/bin/blue-tape.js -- 'dist/**/*.spec.js' | tap-spec",
    "test": "npm run build && npm run lint && npm run test-cov",
    "prepublish": "typings install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/blakeembrey/exif-date.git"
  },
  "keywords": [
    "exif",
    "date",
    "parse",
    "format"
  ],
  "author": {
    "name": "Blake Embrey",
    "email": "hello@blakeembrey.com",
    "url": "http://blakeembrey.me"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/blakeembrey/exif-date/issues"
  },
  "homepage": "https://github.com/blakeembrey/exif-date",
  "devDependencies": {
    "blue-tape": "^0.2.0",
    "istanbul": "^0.4.4",
    "tap-spec": "^4.1.1",
    "tslint": "^3.13.0",
    "tslint-config-standard": "^1.3.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.0"
  }
}
